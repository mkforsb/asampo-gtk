                              <property name="column">0</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="GtkEntry" id="source_add_fs_name_entry">
                            <property name="placeholder_text">Drums</property>
                            <property name="width_request">250</property>
                            <property name="margin_top">10</property>
                            <property name="margin_start">10</property>
                            <property name="margin_end">10</property>
                            <layout>
                              <property name="row">0</property>
                              <property name="column">1</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Path:</property>
                            <property name="xalign">0.0</property>
                            <property name="margin_top">10</property>
                            <property name="margin_start">10</property>
                            <property name="margin_end">10</property>
                            <layout>
                              <property name="row">1</property>
                              <property name="column">0</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="GtkEntry" id="source_add_fs_path_entry">
                            <property name="placeholder_text">/home/user/samples/drums</property>
